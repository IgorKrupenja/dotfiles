{
  // Place your snippets for dart here. Each snippet is defined under a snippet name and has a prefix, body and
  // description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
  // $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
  // same ids are connected.
  // Example:
  // "Print to console": {
  // 	"prefix": "log",
  // 	"body": [
  // 		"console.log('$1');",
  // 		"$2"
  // 	],
  // 	"description": "Log output to console"
  // }
  "Stateful widget": {
    "prefix": "stful",
    "body": [
      "import 'package:flutter/material.dart';",
      "",
      "class $1 extends StatefulWidget {",
      "  const $1();",
      "",
      "  @override",
      "  _$1State createState() => _$1State();",
      "}",
      "",
      "class _$1State extends State<$1> {",
      "  @override",
      "  Widget build(BuildContext context) {",
      "    return Container(",
      "      $2",
      "    );",
      "  }",
      "}"
    ],
    "description": "Stateful widget"
  },
  "Stateless widget": {
    "prefix": "stless",
    "body": [
      "import 'package:flutter/material.dart';",
      "",
      "class $1 extends StatelessWidget {",
      "  const $1();",
      "",
      "  @override",
      "  Widget build(BuildContext context) {",
      "    return Container(",
      "      $2",
      "    );",
      "  }",
      "}"
    ],
    "description": "Stateless widget"
  },
  "Size scaling for WoM project": {
    "prefix": "scale",
    "body": ["ScaleUtil.scale($1)"],
    "description": "Size scaling for WoM project"
  },
  "Horizontal SizedBox with scaling": {
    "prefix": "hsized",
    "body": ["SizedBox(height: ScaleUtil.scale($1)),"],
    "description": "Horizontal SizedBox with scaling"
  },
  "Vertical SizedBox with scaling": {
    "prefix": "vsized",
    "body": ["SizedBox(width: ScaleUtil.scale($1)),"],
    "description": "Vertical SizedBox with scaling"
  }
}
